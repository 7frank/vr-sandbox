{
  "globals": {
    "AFRAME": true,
    "THREE": true,
    "CANNON": true
  },
  "extends": "standard",
  "env": {
    "amd": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "rules": {
    "semi": [2, "always"],
    "no-return-assign":0,
    "no-extra-semi": 2,
     "no-return-assign": 1,
    "no-tabs": 0,
    "no-unused-vars":0,
    "one-var":0,
    "no-mixed-spaces-and-tabs":0,
    "camelcase":0,
    "eqeqeq":0,
    "no-redeclare":0,
    "no-new":0,
    "no-use-before-define":0,
    "standard/object-curly-even-spacing":0,
    "semi-spacing": [2, {
      "before": false,
      "after": true
      }
    ]
  },
  "parser": "babel-eslint"
}
